<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<arg name="model" />

	<param name="robot_description" textfile="$(find ros_manipulator_project)/urdf/manipulator_v2.urdf" />
	<!--arg name="model" default="$(find ros_manipulator_project)/urdf/manipulator.urdf"/-->

	 <!-- Setting gui parameter to true for display joint slider -->
	<arg name="gui" default="true"/>
	<arg name="rvizconfig" default="$(find ros_manipulator_project)/rviz/urdf.rviz" />

	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
        	<param name="use_gui" value="gui"/>
    	</node>

    	<node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" required="true" />
</launch>
